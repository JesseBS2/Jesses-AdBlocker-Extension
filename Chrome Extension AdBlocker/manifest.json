{
  "name": "JS2 Ad-blocker",
  "version": "2.0.0",
  "manifest_version": 3,
  "description": "Jesse's Ad Blocker for Google Chrome.",
  "permissions": [
    "activeTab",
    "scripting",
    "tabs"
  ],
  "background": {
    "service_worker": "background_script.js"
  },
  "host_permissions": [
    "*://*/*"
  ]
}